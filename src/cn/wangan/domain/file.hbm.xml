<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.wangan.domain">

	<class name="UploadFiles" table="file" lazy="false">
		<id name="id" type="java.lang.Long" length="100">
            <generator class="native"/>
		</id>
		<property name="uploadFileName" type="java.lang.String" length="100">	</property>
		<property name="uploadRealName" type="java.lang.String" length="100"></property>
		<property name="uploadContentType" type="java.lang.String" length="100"></property>

	</class>
	
</hibernate-mapping>
